sil = all;

xwip;
tgr/;
mxram 250;
p bom2017.tnt; 
collapse - ;
hold 12500/12500;



agr - . ;

	p new_nondep.tre; 
		pcrprune /\ >0 ;  
		tgr =0 (ew_nondep) . ;
		tv > ;
		k0 ;


	p new_dep.tre; 
		pcrprune /\ >1 ; 
		tgr =1 (ew_dep) . ;
		tv > ;
		k0 ;

	p k5-dep.tre; 
		pcrprune /\ >2 ; 
		tgr =2 (k5_dep) . ;
		tv > ;
		k0 ;

	p k10-dep.tre; 
		pcrprune /\ >3 ; 
		tgr =3 (k10_dep) . ;
		tv > ;
		k0 ;


	p k15-dep.tre;
		pcrprune /\ >4 ; 
		tgr =4 (k15_dep) . ;
		tv > ;
		k0 ;


	p k20-dep.tre; 
		pcrprune /\ >5 ; 
		tgr =5 (k20_dep) . ;
		tv > ;
		k0 ;



/* topological congruence relative to a tree of reference */

var :
reftree_gr
prop_comnod[5]
small_size
simil[5]
count
;

	/*	simil first	*/
tv < ;
set reftree_gr 0 ;


log tables.csv;
