	quote &10 Tree #1 (GROUP #1) &10 ; 
	taxonomy &#1
	=Guaireaceae =Itopsidemoideae =Guaireoideae
	=Osmundaceae =Thamnopteroideae =Osmundoideae
	;
	stop

